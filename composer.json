{
    "name": "gwworg/contao-Cal-Journey-bundle",
    "type": "contao-bundle",
    "description": "Erweiterung fuer Calendar",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Manfred Gipp",
            "homepage": "https://github.com/gwworg"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "4.4.*",
	"contao/calendar-bundle": "4.4.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^5.7.26",
        "symfony/phpunit-bridge": "^3.2"
    },
    "extra": {
        "contao-manager-plugin": "Gwworg\\ContaoCalJourneyBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Gwworg\\ContaoCalJourneyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gwworg\\ContaoJourneyBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/gwworg/contao-cal-journey-bundle/issues",
        "source": "https://github.com/gwworg/contao-cal-journey-bundle"
    }
}
